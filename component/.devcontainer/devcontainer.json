{
	"name": "C++",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "debian-11"
		}
	},
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {},
	"extensions": [
		"ms-vscode.cpptools",
		"ms-vscode.cmake-tools",
		"twxs.cmake"
	],
	"remoteUser": "vscode",
	"features": {
		"docker-from-docker": "20.10",
		"git": "os-provided",
		"git-lfs": "latest",
		"github-cli": "latest",
		"azure-cli": "latest",
		"sshd": "latest",
		"java": "11"
	}
}